resource_name = "Camera"
script/source = "extends Camera2D

@export var zoom_speed : float;
@export var zoom_levels : Array
var zoom_progress = 0;

func _ready():
	zoom.x = zoom_levels[0]
	zoom.y = zoom_levels[0]
	
func _next_zoom():
	zoom_progress += 1
	if zoom_progress >= zoom_levels.size():
		return 
	zoom.x = zoom_levels[zoom_progress]
	zoom.y = zoom_levels[zoom_progress]
	
func _get_zoom():
	return zoom_levels[zoom_progress]
"

