[sub_resource type="GDScript" id="GDScript_q6n73"]
resource_name = "tutorial_manager"
script/source = "extends Main

var press_progress = 0
@onready var pressed = [false, false, false, false]
@onready var progress = 0
@onready var move_sprite = get_node(\"MoveSprite\")
@onready var shoot_sprite = get_node(\"ShootSprite\")
@onready var goal_sprite = get_node(\"GoalSprite\")
@onready var enemy

func _ready():
	shoot_sprite.hide()
	goal_sprite.hide()

func _process(delta):
	_move_phase()


func _move_phase():
	_check_input(\"up\", 0)
	_check_input(\"down\" ,1)
	_check_input(\"left\" ,2)
	_check_input(\"right\" ,3)
	
	if press_progress == 4:
		set_process(false)
		move_sprite.hide()
		shoot_sprite.show()
		
		enemy = load(\"res://Scenes/Objects/Characters/enemy.tscn\").instantiate()
		enemy.set_process(false)
		enemy.set_physics_process(false)
		enemy.position = Vector2i(500, 800)
		add_child(enemy)

func _check_input(direction, index):
	if pressed[index] == false:
		pressed[index] = Input.is_action_pressed(direction)
		if pressed[index] == true:
			press_progress += 1
	
func _remove_enemy(e):
	enemy.queue_free()
	shoot_sprite.hide()
	goal_sprite.show()
	await get_tree().create_timer(5.0).timeout
	get_tree().change_scene_to_file(\"res://Scenes/Menus/main_menu.tscn\")
	
"

